{
    "name": "omegacode/jwt-secured-api-skeleton",
    "type": "project",
    "description": "This is a simple jwt based API skeleton to kick start your API development.",
    "homepage": "https://github.com/0m3gaC0d3/jwt-secured-api-skeleton",
    ""
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "php": ">=7.4",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1",
        "doctrine/dbal": "^2.10",
        "symfony/dotenv": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/config": "^5.0",
        "symfony/yaml": "^5.0",
        "doctrine/annotations": "^1.8",
        "http-interop/http-factory-guzzle": "^1.0",
        "lcobucci/jwt": "^3.3",
        "cakephp/chronos": "^2.0",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "omegacode/t3-debugger-utility-standalone": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -f ./Install/build.php"
        ]
    }
}
